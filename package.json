{
  "name": "aether",
  "displayName": "Aether",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@babycommando/entity-db": "^1.0.11",
    "@plasmohq/storage": "^1.15.0",
    "@xenova/transformers": "^2.17.2",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "https://huggingface.co/*",
      "https://*.huggingface.co/*",
      "https://hf.co/*",
      "https://*.hf.co/*",
      "https://cdn.jsdelivr.net/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; connect-src 'self' ws://localhost:* https://huggingface.co https://*.huggingface.co https://hf.co https://*.hf.co https://cdn.jsdelivr.net; object-src 'self'"
    }
  }
}